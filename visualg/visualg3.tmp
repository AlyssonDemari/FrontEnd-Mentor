Algoritmo "semnome"
Var
   C, T1, T2, T3: inteiro
procedimento proximofibonacci(var A, B:inteiro)
var C:inteiro
inicio
      C := A + B
      escreva(C)
      A := B
      B := C
fimprocedimento
Inicio
      T1 <- 0
      escreva(T1)
      T2 <- 1
      escreva(T2)
      para C := 3 ate 10 faca
         proximofibonacci(T1, T2)
      fimpara
Fimalgoritmo